{% extends 'dashboard.twig' %}

{% block content %}

    <div class="reportContent">
        {% if (menu is defined and menu) %}
            {% include "@CoreHome/_menu.twig" %}
        {% endif %}

        {% if rows|length > 0 %}
            {%  for row in rows %}
                {{ row['label'] }} - Visits: {{ row['nb_visits'] }} - Actions: {{ row['nb_actions'] }} <br>
            {% endfor %}
        {% else %}
            Nothing to report. Go your way.
        {% endif %}
    </div>

{% endblock %}